@model IEnumerable<EmployeeManagement2.Models.SuneetEmployee>;
@**@
<h4>
    <a asp-action="Create">Add Employee</a>
</h4>
<div><p><b>Search Employee</b></p></div>
<div class="form -group">
    <form asp-action="SearchProducts">
    <input type=text name="keyword" id="keyword">
    <input type="submit" value="Search" class="btn btn-primary btn-sm" />
    </form>
</div>
<table class="table">
<tr>
    <th>EmployeeId</th>
    <th>FirstName</th>
    <th>LastName</th>
    
</tr>
@foreach(var item in Model)
{
    <tr>
        <td>@item.EmployeeId</td>
        <td>@item.FirstName</td>
        <td>@item.LastName</td>
        <td> <a asp-action="Edit"asp-route-id="@item.EmployeeId">Edit</a></td>
        <td> <a asp-action="Delete"asp-route-id="@item.EmployeeId">Delete</a></td>
        <td> <a asp-action="Details"asp-route-id="@item.EmployeeId">Details</a></td>
        </tr>


}
</table>
 
